{
    "name": "spryker-eco/unzer-rest-api",
    "type": "library",
    "description": "UnzerRestApi module",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "spryker-eco/unzer": "dev-feature/cc-14610/dev-unzer-psp as 1.0",
        "spryker/glue-application": "^1.36.0",
        "spryker/glue-application-extension": "^1.5.0",
        "spryker/kernel": "^3.30.0",
        "spryker/symfony": "^3.7.0"
    },
    "require-dev": {
        "codeception/module-asserts": "^1.3.0",
        "phpstan/phpstan": "^1.2.0",
        "spryker/application": "^3.0.0",
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerEco\\": "src/SprykerEco/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "stan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/codecept run",
        "lowest": "validate-prefer-lowest",
        "lowest-setup": "composer update --prefer-lowest --prefer-stable --prefer-dist --no-interaction && cp composer.json composer.backup && composer require --dev dereuromark/composer-prefer-lowest && mv composer.backup composer.json"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/spryker-eco/unzer.git"
        },
        {
            "type": "git",
            "url": "https://github.com/spryker-eco/unzer-api.git"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
